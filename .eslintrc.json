{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        // "google",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }, 
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": ["error", 2],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "double"],
        "semi": ["error", "always"]
    }
}
